{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/students(.*)", "dest": "/server.js" },
    { "src": "/api/tutors(.*)", "dest": "/server.js" },
    { "src": "/(.*)", "dest": "/server.js" }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/" }]
}
